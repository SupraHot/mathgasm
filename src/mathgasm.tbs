class mathgasm_vec3 {
   x: float32;
   y: float32;
   z: float32;
   
   constructor( x : float32, y : float32, z : float32 ) : mathgasm_vec3 {
       this.x = x;
       this.y = y;
       this.z = z;
       return this;
   }
}

export function new_mathmgasm_vec3( x : float32, y : float32, z : float32 ) : mathgasm_vec3 {
    return new mathgasm_vec3(x, y, z);
}

export function destroy_mathmgasm_vec3( a : mathgasm_vec3 ) : void {
    delete a;
}